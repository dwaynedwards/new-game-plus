{
  "name": "new-game-plus",
  "version": "0.0.1",
  "description":
    "A MERN stack application that retrieves Xbox One and Playstation 4 game information from a database and displays it to the user.",
  "engines": {
    "node": ">= 7.6.0"
  },
  "author":
    "Dwayne Edwards <dwayne@dwaynedwards.com> (https://dwaynedwards.com)",
  "license": "MIT",
  "scripts": {
    "server": "nodemon --exec babel-node server.js --ignore public",
    "client": "webpack -w --mode development",
    "start":
      "concurrently \"npm run server\" \"npm run client\" -n \"express,react\" -p name"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.1.0",
    "pug": "^2.0.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "semantic-ui-react": "^0.80.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "sass-loader": "^7.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
